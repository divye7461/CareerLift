generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ResumeAnalysis{
  id String @id @default(uuid())
  ats_score Float 
  suggestions  String[]
  flaws        String[]
  key_improvements    String[]
  createdAt    DateTime @default(now())
}


